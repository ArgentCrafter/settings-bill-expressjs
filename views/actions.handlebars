<div class="row"></div>
<form action="/" method="GET">
<h1>Actions</h1>

<table>
  <tr>
    <th>Type</th>
    <th>Price</th>
    <th>Timestamp</th>
  </tr>
  {{#each actions}}
  <tr>
      <td>{{type}}</td>
      <td>{{cost}}</td>
      <td>{{timestampago}}</td>
  </tr>
  {{/each}}
</table>
<hr>
<button class="button-primary" type="submit" name="backBtn">Back</button>
</form>